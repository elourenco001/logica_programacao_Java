Algoritmo "crescente"

Var


x , y : inteiro

Inicio

escreval("Digite dois numeros: ")
leia(x)
leia(y)

enquanto x <> y faca

  se x < y entao
   escreval("CRESCENTE")
   senao
   escreval("DECRESCENTE")
   fimse
   
   escreval("Digite outro dois numeros: ")
   leia(x)
   leia(y)
   

fimenquanto

//escreval("X: ",x)
//escreval("Y: ",y)

Fimalgoritmo